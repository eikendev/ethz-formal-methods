restriction Equality:
  "∀ x y #i. (Eq( x, y ) @ #i) ⇒ (x = y)"
  // safety formula

lemma executability:
  exists-trace
  "∃ A B Km #i #j.
    (((((Secret( A, Km ) @ #i) ∧ (Secret( B, Km ) @ #j)) ∧
       (∀ id1 id2 #k1 #k2.
         ((Create( id1, 'I' ) @ #k1) ∧ (Create( id2, 'I' ) @ #k2)) ⇒
         (id1 = id2))) ∧
      (∀ id1 id2 #k1 #k2.
        ((Create( id1, 'R' ) @ #k1) ∧ (Create( id2, 'R' ) @ #k2)) ⇒
        (id1 = id2))) ∧
     (¬(A = B))) ∧
    (¬(∃ X #k. Reveal( X ) @ #k))"
/*
guarded formula characterizing all satisfying traces:
"∃ A B Km #i #j.
  (Secret( A, Km ) @ #i) ∧ (Secret( B, Km ) @ #j)
 ∧
  (∀ id1 id2 #k1 #k2.
    (Create( id1, 'I' ) @ #k1) ∧ (Create( id2, 'I' ) @ #k2)
   ⇒
    id1 = id2) ∧
  (∀ id1 id2 #k1 #k2.
    (Create( id1, 'R' ) @ #k1) ∧ (Create( id2, 'R' ) @ #k2)
   ⇒
    id1 = id2) ∧
  (¬(A = B)) ∧
  (∀ X #k. (Reveal( X ) @ #k) ⇒ ⊥)"
*/
simplify
solve( Secret( A, Km ) @ #i )
  case A_4
  solve( St_A_3( A, id, B, sk, pk(x), x.1, gb, h(z) ) ▶₀ #i )
    case A_3
    solve( Secret( B, h(z) ) @ #j )
      case B_3
      solve( St_B_2( B, id.1, A.2, sk.2, pk(x.1), ga, y ) ▶₀ #j )
        case B_2
        solve( !KU( sign(<'g'^~x, gb>, ~sk.1) ) @ #vk.3 )
          case B_4
          solve( !KU( sign(<'g'^~y, 'g'^~x>, ~sk.2) ) @ #vk.11 )
            case A_3
            solve( !KU( senc(<'1', $A.1>, h('g'^(~x*~y))) ) @ #vk.6 )
              case B_4
              solve( !KU( pk(~sk.1) ) @ #vk.9 )
                case B_4
                solve( !KU( senc(<'0', $A>, h('g'^(~x*~y))) ) @ #vk.13 )
                  case A_3
                  solve( !KU( pk(~sk) ) @ #vk.14 )
                    case A_3
                    solve( !KU( 'g'^~x ) @ #vk.15 )
                      case A_1
                      solve( !KU( 'g'^~y ) @ #vk.14 )
                        case B_2
                        SOLVED // trace found
                      qed
                    qed
                  qed
                qed
              qed
            qed
          qed
        qed
      qed
    qed
  qed
qed

lemma injectiveagreementINITIATOR:
  all-traces
  "∀ a b Km #i.
    (Commit( a, b, <'I', 'R', Km> ) @ #i) ⇒
    ((∃ #j.
       (Running( b, a, <'I', 'R', Km> ) @ #j) ∧
       (¬(∃ a2 b2 #i2.
           (Commit( a2, b2, <'I', 'R', Km> ) @ #i2) ∧ (¬(#i2 = #i))))) ∨
     (∃ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i)))"
/*
guarded formula characterizing all counter-examples:
"∃ a b Km #i.
  (Commit( a, b, <'I', 'R', Km> ) @ #i)
 ∧
  (∀ #j.
    (Running( b, a, <'I', 'R', Km> ) @ #j)
   ⇒
    ∃ a2 b2 #i2.
     (Commit( a2, b2, <'I', 'R', Km> ) @ #i2) ∧ ¬(#i2 = #i)) ∧
  (∀ X #r. (Reveal( X ) @ #r) ∧ (Honest( X ) @ #i) ⇒ ⊥)"
*/
simplify
solve( Commit( a, b, <'I', 'R', Km> ) @ #i )
  case A_4
  solve( St_A_3( a, id, b, sk, pk(x), x.1, gb, h(z) ) ▶₀ #i )
    case A_3
    solve( !KU( sign(<'g'^~x, gb>, ~sk.1) ) @ #vk.3 )
      case A_3
      solve( !KU( senc(<'1', $A.1>, h('g'^(~x*~x.1))) ) @ #vk.5 )
        case B_4
        solve( splitEqs(4) )
          case split
          solve( !KU( sign(<'g'^~y, 'g'^(~x*~x.1*inv(~y))>, ~sk.4)
                 ) @ #vk.12 )
            case c_sign
            solve( !KU( ~sk.4 ) @ #vk.18 )
              case Reveal_SK
              solve( !KU( 'g'^(~x*~x.1*inv(~y)) ) @ #vk.17 )
                case A_1_case_1
                by solve( !KU( ~x ) @ #vk.21 )
              next
                case A_1_case_2
                by solve( !KU( ~x.1 ) @ #vk.21 )
              next
                case c_exp
                by solve( !KU( ~x ) @ #vk.22 )
              qed
            qed
          qed
        qed
      next
        case c_senc
        solve( !KU( h('g'^(~x*~x.1)) ) @ #vk.10 )
          case c_h
          solve( !KU( 'g'^(~x*~x.1) ) @ #vk.12 )
            case A_1_case_1
            by solve( !KU( ~x ) @ #vk.13 )
          next
            case A_1_case_2
            by solve( !KU( ~x.1 ) @ #vk.13 )
          next
            case c_exp
            by solve( !KU( ~x ) @ #vk.15 )
          qed
        qed
      qed
    next
      case B_4
      solve( !KU( sign(<'g'^~y, 'g'^~x>, ~sk.2) ) @ #vk.11 )
        case A_3
        solve( (#i2 < #i)  ∥ (#i < #i2) )
          case case_1
          solve( Commit( a2, b2, <'I', 'R', h('g'^(~x*~y))> ) @ #i2 )
            case A_4
            solve( St_A_3( a2, id.1, b2, sk.2, pk(x.1), x.2, gb, h('g'^(~x*~y))
                   ) ▶₀ #i2 )
              case A_3
              solve( splitEqs(3) )
                case split_case_1
                by contradiction /* cyclic */
              next
                case split_case_2
                solve( !KU( sign(<'g'^~x.1, 'g'^(~x*~y*inv(~x.1))>, ~sk.3)
                       ) @ #vk.19 )
                  case c_sign
                  solve( !KU( ~sk.3 ) @ #vk.25 )
                    case Reveal_SK
                    solve( !KU( 'g'^(~x*~y*inv(~x.1)) ) @ #vk.24 )
                      case A_1
                      by solve( !KU( ~y ) @ #vk.28 )
                    next
                      case B_2
                      by solve( !KU( ~x ) @ #vk.28 )
                    next
                      case c_exp
                      by solve( !KU( ~x ) @ #vk.29 )
                    qed
                  qed
                qed
              qed
            qed
          qed
        next
          case case_2
          solve( Commit( a2, b2, <'I', 'R', h('g'^(~x*~y))> ) @ #i2 )
            case A_4
            solve( St_A_3( a2, id.1, b2, sk.2, pk(x.1), x.2, gb, h('g'^(~x*~y))
                   ) ▶₀ #i2 )
              case A_3
              solve( splitEqs(3) )
